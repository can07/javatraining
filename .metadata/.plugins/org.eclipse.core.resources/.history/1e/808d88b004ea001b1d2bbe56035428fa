package Test;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		int n, i, j,sum=0,count=0;
		//int flag=0;
		Scanner scan = new Scanner(System.in);
		n=scan.nextInt();
		scan.close();
		if(n<0) {
			System.out.println("invalid array size");
			System.exit(0);
			
		}
		else 
		{
			int a[] = new int[n];
			for(i=0; i<n;i++) {
				a[i]=scan.nextInt();
				if(a[i]<0)
				{
					//flag=1;
					System.out.println("invalid input");
					System.exit(0);
				}
			}
			//if(flag!=1)
			//{
			for(i=0;i<n;i++)
			{
				count=0;
				for(j=1;j<=a[i];j++) {
					if(a[i]%j==0)
						count ++;
					
				}
				if(count==2)
					sum=sum+a[i];
				
			}
			System.out.println(sum);
			//}
		}
	}
}
